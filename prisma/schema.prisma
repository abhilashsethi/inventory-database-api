generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
}

model Supplier {
  id        Int         @id @default(autoincrement())
  name      String
  city      String
  inventory Inventory[]
}

model Inventory {
  id          Int      @id @default(autoincrement())
  productName String
  quantity    Int
  price       Float
  supplierId  Int
  supplier    Supplier @relation(fields: [supplierId], references: [id])
}
